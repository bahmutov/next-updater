{
  "name": "next-updater",
  "version": "0.0.1",
  "description": "Dependable and safe automatic dependency updater for Nodejs packages",
  "main": "index.js",
  "scripts": {
    "test": "grunt && npm run unit && npm run e2e",
    "unit": "node node_modules/gt src/**/test/*.js --output",
    "e2e": "node node_modules/gt test/*.js --output"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bahmutov/next-updater.git"
  },
  "keywords": [
    "update",
    "service",
    "automatic",
    "dependency",
    "npm",
    "package",
    "updater"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "check-types": "1.4.0",
    "ggit": "0.1.1",
    "optimist": "0.6.1",
    "q": "2.0.2"
  },
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-bump": "0.0.16",
    "grunt-complexity": "0.2.0",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-deps-ok": "0.5.1",
    "grunt-jsonlint": "~1.0.4",
    "grunt-nice-package": "0.9.2",
    "grunt-readme": "~0.4.5",
    "gt": "0.8.44",
    "jshint-stylish": "1.0.0",
    "matchdep": "~0.3.0",
    "time-grunt": "1.0.0"
  }
}
